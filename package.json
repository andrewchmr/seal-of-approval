{
  "name": "seal-of-approval",
  "version": "0.0.0",
  "private": true,
  "description": "Github action that gives seal of approval to your PR.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewchmr/seal-of-approval.git"
  },
  "keywords": [
    "actions",
    "seal",
    "approval",
    "github"
  ],
  "author": {
    "name": "Andriy Chemerynskiy",
    "url": "https://andrewchmr.com/"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@vercel/ncc": "^0.25.0",
    "eslint": "^7.7.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.2"
  }
}